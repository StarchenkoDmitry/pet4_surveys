generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User{
  id  String @id @default(uuid())
  name String?

  providers GithubProvider[]
}

model GithubProvider{
  id  String @id @default(uuid())
  token String

  userId String
  user User @relation(fields: [userId],references: [id])
}

model Template{
  id  String @id @default(uuid())
  name String
  testText String
}
